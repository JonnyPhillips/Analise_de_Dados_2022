@article{Elkins2004,
abstract = {One of the most important developments over the past three decades has been the spread of liberal{\$}\backslash{\$}neconomic ideas and policies throughout the world. These policies have affected the lives of millions{\$}\backslash{\$}nof people, yet our most sophisticated political economy models do not adequately capture{\$}\backslash{\$}ninfluences on these policy choices. Evidence suggests that the adoption of liberal economic practices{\$}\backslash{\$}nis highly clustered both temporally and spatially. We hypothesize that this clustering might be due to{\$}\backslash{\$}nprocesses of policy diffusion.We think of diffusion as resulting from one of two broad sets of forces: one{\$}\backslash{\$}nin which mounting adoptions of a policy alter the benefits of adopting for others and another in which{\$}\backslash{\$}nadoptions provide policy relevant information about the benefits of adopting. We develop arguments{\$}\backslash{\$}nwithin these broad classes of mechanisms, construct appropriate measures of the relevant concepts, and{\$}\backslash{\$}ntest their effects on liberalization and restriction of the current account, the capital account, and the{\$}\backslash{\$}nexchange rate regime. Our findings suggest that domestic models of foreign economic policy making{\$}\backslash{\$}nare insufficient. The evidence shows that policy transitions are influenced by international economic{\$}\backslash{\$}ncompetition as well as the policies of a country's sociocultural peers.We interpret the latter influence as a{\$}\backslash{\$}nform of channeled learning reflecting governments' search for appropriate models for economic policy.},
author = {Elkins, Zachary and Simmons, Beth},
journal = {American political science review},
