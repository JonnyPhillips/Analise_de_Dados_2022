@article{Blair2016,
abstract = {Researchers need to select high-quality research designs and communicate those designs clearly to readers. Both tasks are difficult. We provide a framework for formally "declaring" the analytically relevant features of a research design in a demonstrably complete manner, with applications to qualitative, quantitative, and mixed methods research. The approach to design declaration we describe requires defining a model of the world (M), an inquiry (I), a data strategy (D), and an answer strategy (A). Declaration of these features in code provides sufficient information for researchers and readers to use Monte Carlo techniques to diagnose properties such as power, bias, accuracy of qualitative causal inferences, and other "diag-nosands." Ex ante declarations can be used to improve designs and facilitate preregistration, analysis, and reconciliation of intended and actual analyses. Ex post declarations are useful for describing, sharing, reanalyzing, and critiquing existing designs. We provide open-source software, DeclareDesign, to implement the proposed approach. for their many contributions to the broader project. The methods proposed in this paper are implemented in an accompanying open-source software package, DeclareDesign (Blair et al. 2018).},
author = {Blair, Graeme and Cooper, Jasper and Coppock, Alexander and Humphreys, Macartan and Aronow, Peter and Br{\"{u}}ckner, Julian and Hartman, Erin and Green, Donald and Grimmer, Justin and Hansen, Kolby and Jacobs, Alan and Leavitt, Tom and Lin, Winston and Mildenberger, Matto and Orlowski, Matthias and Roberts, Molly and Slough, Tara and Gosha, Syun-Yaev and Wilke, Anna and Yamamoto, Teppei and York, Erin and Young, Lauren and Zhou, Yang-Yang and Correia, Clara and Fultz, Neal and Huang, Sisi and Konrad, Markus and Medina, Lily and Mohanty, Pete and Rudkin, Aaron and Singh, Shikhar and Sonnet, Luke and Ternovski, John},
journal = {Apsa},
title = {{Declaring and Diagnosing Research Designs † meetings for helpful comments. We thank}},
