@article{Gingerich2014a,
abstract = {This paper examines how the adoption of the Australian ballot (AB), and ipso facto, the transition from the nominal to effective secret vote, shaped the nature of party politics in Brazil. To address this issue, the paper utilizes an original dataset—painstakingly assembled from regional electoral archives—containing municipal-level vote returns for federal deputy and senate contests during the period before and after the AB's introduction in Brazil (1958-1962). The dataset exploits the fact that the AB was in- troduced at different times in different states and for different offices in the country, thereby creating an unprecedented opportunity to assess the impact of this institu- tion. Using a triple difference-in-difference procedure, the study finds that the AB: 1) shifted the ideological balance of power from Right to Left; 2) did not greatly weaken the hand of local vote brokers; and 3) greatly increased the proportion of wasted votes cast in elections},
author = {Gingerich, Daniel W},
doi = {10.2139/ssrn.2367695},
issn = {1556-5068},
journal = {Working paper},
keywords = {assessing the impact of,australian ballot in brazil,brazil,clientelism,electoral institutions,institutions cure clientelism,political participa-,the,vote secrecy},
number = {1119908},
title = {{Can Institutions Cure Clientelism? Assessing the Impact of the Australian Ballot in Brazil}},
url = {http://www.ssrn.com/abstract=2367695},
year = {2014}
}
