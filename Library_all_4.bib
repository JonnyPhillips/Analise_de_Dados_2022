journal = {Social Science {\&} Medicine},
keywords = {Delivery of Health Care,Delivery of Health Care: standards,Health Policy,Health Status Indicators,Humans,Models,Public Health,Public Health: statistics {\&} numerical data,Statistical,World Health},
month = {sep},
pages = {21--28},
