month = {may},
number = {5},
pages = {2306--2316},
pmid = {19389989},
title = {{Mathematical optimization to improve cows' artificial insemination services.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3169519{\&}tool=pmcentrez{\&}rendertype=abstract http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3169519{\%}5C{\&}tool=pmcentrez{\%}5C{\&}rendertype=abstract},
