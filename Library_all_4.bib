@article{Fotheringham2011,
abstract = {A set of point events, such as locations of disease cases, accidents, crimes, and emergency calls for instance, usually exhibits a spatial point pattern of random, clustered, or uniform nature. It is important to rely on simple and rapid methods to detect not only whether such patterns exhibit clustering, but also where clustering occurs. In the case of crime mapping, knowing where clusters are located, along with their intensity, can assist in planning safety measures to better protect an area. This article overviews techniques for point pattern analysis, which are usually supported by a geographic information system (GIS). Methods for visual exploration of point patterns and analysis of clustering are discussed. The issue of scale is of paramount importance insofar as it strongly affects the results of point patterns analyses. A sensitivity analysis that evaluates the effect of scale on the resulting point pattern is desirable.},
author = {Fotheringham, A and Brunsdon, Chris and Charlton, Martin},
doi = {10.4135/9781849209755.n6},
journal = {Quantitative Geography},
pages = {130--161},
title = {{Point Pattern Analysis}},
year = {2011}
