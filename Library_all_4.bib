@article{Gerber2008b,
abstract = {We examine the APSR and the AJPS for the presence of publication bias{\$}\backslash{\$}ndue to reliance on the 0.05 significance level. Our analysis employs{\$}\backslash{\$}na broad interpretation of publication bias, whichwe define as the{\$}\backslash{\$}noutcome that occurs when, for whatever reason, publication practices{\$}\backslash{\$}nlead to bias in the published parameter estimates.We examine the{\$}\backslash{\$}neffect of the 0.05 significance level on the pattern of published{\$}\backslash{\$}nfindings using a "caliper" test, a novel method for comparing studies{\$}\backslash{\$}nwith heterogeneous effects, and find that we can reject the hypothesis{\$}\backslash{\$}nof no publication bias at the 1 in 32 billion level. Our findings{\$}\backslash{\$}ntherefore raise the possibility that the results reported in the{\$}\backslash{\$}nleading political science journals may be misleading due to publication{\$}\backslash{\$}nbias. We also discuss some of the reasons for publication bias and{\$}\backslash{\$}npropose reforms to reduce its impact on research.},
author = {Gerber, Alan},
doi = {10.1561/100.00008024},
journal = {Quarterly Journal of Political Science},
number = {3},
pages = {313--326},
title = {{Do Statistical Reporting Standards Affect What Is Published? Publication Bias in Two Leading Political Science Journals}},
volume = {3},
year = {2008}
