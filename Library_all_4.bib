@article{Kennedy2011,
abstract = {OBJECTIVE: To determine the effect of income inequality as measured by the Robin Hood index and the Gini coefficient on all cause and cause specific mortality in the United States.{\$}\backslash{\$}n{\$}\backslash{\$}nDESIGN: Cross sectional ecological study.{\$}\backslash{\$}n{\$}\backslash{\$}nSETTING: Households in the United States.{\$}\backslash{\$}n{\$}\backslash{\$}nMAIN OUTCOME MEASURES: Disease specific mortality, income, household size, poverty, and smoking rates for each state.{\$}\backslash{\$}n{\$}\backslash{\$}nRESULTS: The Robin Hood index was positively correlated with total mortality adjusted for age (r = 0.54; P {\textless}0.05). This association remained after adjustment for poverty (P {\textless}0.007), where each percentage increase in the index was associated with' an increase in the total mortality of 21.68 deaths per 100,000. Effects of the index were also found for infant mortality (P = 0.013); coronary heart disease (P = 0.004); malignant neoplasms (P = 0.023); and homicide (P {\textless}0.001). Strong associations were also found between the index and causes of death amenable to medical intervention. The Gini coefficient showed very little correlation with any of the causes of death.{\$}\backslash{\$}n{\$}\backslash{\$}nCONCLUSION: Variations between states in the inequality of income were associated with increased mortality from several causes. The size of the gap between the wealthy and less well off--as distinct from the absolute standard of living enjoyed by the poor--seems to matter in its own right. The findings suggest that policies that deal with the growing inequities in income distribution may have an important impact on the health of the population.},
author = {Kennedy, B P and Kawachi, I and Prothrow-Stith, D},
doi = {10.1136/bmj.312.7037.1004},
issn = {0959-8138},
journal = {Bmj},
number = {7037},
pages = {1004--1007},
title = {{Income distribution and mortality: cross sectional ecological study of the Robin Hood index in the United States}},
volume = {312},
year = {2011}
}
