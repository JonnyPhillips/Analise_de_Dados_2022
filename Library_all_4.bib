@book{Imai2017,
abstract = {Within the past several years, a considerable body of research on social capital has emerged in public health.{\$}\backslash{\$}nAlthough offering the potential for new insights into how community factors impact health and well being, this research{\$}\backslash{\$}nhas received criticism for being undertheorized and methodologically ﬂawed. In an effort to address some of these{\$}\backslash{\$}nlimitations, this paper applies Pierre Bourdieu's (1986) [Bourdieu, P. (1986). Handbook of theory and research for the{\$}\backslash{\$}nsociology of education (pp. 241–258). New York: Greenwood] social capital theory to create a conceptual model of{\$}\backslash{\$}nneighborhood socioeconomic processes, social capital (resources inhered within social networks), and health. After{\$}\backslash{\$}nbrieﬂy reviewing the social capital conceptualizations of Bourdieu and Putnam, I attempt to integrate these authors'{\$}\backslash{\$}ntheories to better understand how social capital might operate within neighborhoods or local areas. Next, I describe a{\$}\backslash{\$}nconceptual model that incorporates this theoretical integration of social capital into a framework of neighborhood{\$}\backslash{\$}nsocial processes as health determinants. Discussion focuses on the utility of this Bourdieu-based neighborhood social{\$}\backslash{\$}ncapital theory and model for examining several under-addressed issues of social capital in the neighborhood effects{\$}\backslash{\$}nliterature and generating speciﬁc, empirically testable hypotheses for future research.{\$}\backslash{\$}nr 2005 Elsevier Ltd. All rights reserved.},
archivePrefix = {arXiv},
