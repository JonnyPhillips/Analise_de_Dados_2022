@book{Institucionais2014,
abstract = {Este livro tem o objetivo de aprofundar o debate sobre o Estado e desenvolvimento no limiar do s{\'{e}}culo XXI por meio da an{\'{a}}lise das capacidades estatais de implementa{\c{c}}{\~{a}}o de pol{\'{i}}ticas p{\'{u}}blicas no Brasil democr{\'{a}}tico. O momento hist{\'{o}}rico atual marca um encontro entre uma postura ativa do Estado brasileiro e a vig{\^{e}}ncia de institui{\c{c}}{\~{o}}es democr{\'{a}}ticas estabelecidas pela Constitui{\c{c}}{\~{a}}o Federal de 1988. Este encontro suscita um conjunto de quest{\~{o}}es, entre elas a capacidade do Poder Executivo de implementar suas pol{\'{i}}ticas envolvendo m{\'{u}}ltiplos atores e interesses, sem violar os direitos e as institui{\c{c}}{\~{o}}es garantidas por um regime democr{\'{a}}tico e pluralista. Isso demanda novas capacidades do Estado, que v{\~{a}}o al{\'{e}}m das necessidades de uma burocracia profissional e aut{\^{o}}noma, que possa tra{\c{c}}ar estrat{\'{e}}gias com os atores privados sem ser capturada. Ou seja, mais que as capacidades t{\'{e}}cnicas e administrativas exigidas de uma burocracia weberiana cl{\'{a}}ssica. Este livro apresenta a tese de que, no contexto democr{\'{a}}tico atual, caracterizado pela exist{\^{e}}ncia e o funcionamento de institui{\c{c}}{\~{o}}es representativas, participativas e de controles burocr{\'{a}}ticos, s{\~{a}}o necess{\'{a}}rias tamb{\'{e}}m capacidades pol{\'{i}}ticas dos agentes do Estado para a produ{\c{c}}{\~{a}}o de pol{\'{i}}ticas p{\'{u}}blicas. No debate da literatura sobre o tema, encontram-se desde posi{\c{c}}{\~{o}}es que advogam pela sinergia existente entre democracia e pol{\'{i}}ticas de car{\'{a}}ter desenvolvimentista, ressaltando a qualidade e legitimidade de decis{\~{o}}es compartilhadas, at{\'{e}} argumentos que enfatizam os conflitos e obst{\'{a}}culos aos processos de desenvolvimento acelerado decorrentes da incorpora{\c{c}}{\~{a}}o de amplo conjunto de interesses nos processos decis{\'{o}}rios, com a multiplica{\c{c}}{\~{a}}o dos pontos de veto. As institui{\c{c}}{\~{o}}es democr{\'{a}}ticas imp{\~{o}}em restri{\c{c}}{\~{o}}es {\`{a}} implementa{\c{c}}{\~{a}}o de pol{\'{i}}ticas p{\'{u}}blicas, gerando inefici{\^{e}}ncias e impasses? Como conciliar as distintas dimens{\~{o}}es do desenvolvimento – econ{\^{o}}mica, social, pol{\'{i}}tica, ambiental –, respeitando-se os m{\'{u}}ltiplos interesses dos atores que comp{\~{o}}em a sociedade? Para responder a tais quest{\~{o}}es, o Ipea realizou, por meio da colabora{\c{c}}{\~{a}}o entre pesquisadores do instituto e docentes de universidades brasileiras e estrangeiras, projeto de pesquisa que deu origem a este livro. A pesquisa teve como objetivo analisar em profundidade os arranjos institucionais de pol{\'{i}}ticas p{\'{u}}blicas representativas dos atuais esfor{\c{c}}os do governo em promover o desenvolvimento. Por arranjos institucionais, definiu-se o conjunto de regras, mecanismos e processos que definem a forma particular como se coordenam atores e interesses na implementa{\c{c}}{\~{a}}o de cada pol{\'{i}}tica. No decorrer da pesquisa foram desenvolvidos novos conceitos e modelos anal{\'{i}}ticos, apresentados neste volume, que permitiram compreender a opera{\c{c}}{\~{a}}o de atores no interior do Estado e como as institui{\c{c}}{\~{o}}es democr{\'{a}}ticas se fazem presentes nos casos estudados, extraindo-se infer{\^{e}}ncias e proposi{\c{c}}{\~{o}}es pass{\'{i}}veis de serem utilizadas para an{\'{a}}lise de outros casos. Os achados indicam que o aventado trade-off entre a a{\c{c}}{\~{a}}o do Estado e institui{\c{c}}{\~{o}}es democr{\'{a}}ticas {\'{e}} menos prevalente do que se imaginava. Os casos estudados demonstraram que a condu{\c{c}}{\~{a}}o de pol{\'{i}}ticas de desenvolvimento em ambiente democr{\'{a}}tico imp{\~{o}}e – isso sim – novas capacidades por parte das burocracias p{\'{u}}blicas, tanto no que se refere {\`{a}}s habilidades t{\'{e}}cnicas de formula{\c{c}}{\~{a}}o quanto no que concerne {\`{a}}s compet{\^{e}}ncias pol{\'{i}}ticas de implementa{\c{c}}{\~{a}}o. Espera-se que este material possa contribuir para a {\'{a}}rea de conhecimento sobre pol{\'{i}}ticas p{\'{u}}blicas, fornecendo elementos n{\~{a}}o s{\'{o}} para um programa de pesquisa sobre capacidades estatais e democracia, mas tamb{\'{e}}m para a{\c{c}}{\~{a}}o p{\'{u}}blica de qualidade neste Brasil de limiar de s{\'{e}}culo.},
author = {Institucionais, Arranjos and P{\'{u}}blicas, De Pol{\'{i}}ticas},
isbn = {9788578111991},
keywords = {1. Pol{\'{i}}tica de Desenvolvimento. 2. Pol{\'{i}}ticas P{\'{u}}bli,Alexandre de {\'{A}}vila. II. Pires,Roberto Rocha C. III. Instituto de Pesquisa Econ{\^{o}}m},
pages = {385},
title = {{Capacidades Estatais e Democracia: Arranjos Institucionais de Pol{\'{i}}ticas P{\'{u}}blicas}},
year = {2014}
}
