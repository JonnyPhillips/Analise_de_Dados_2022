issn = {00104140},
journal = {Comparative Political Studies},
keywords = {government performance,india,party competition,party systems,public goods},
number = {2},
pages = {152--187},
title = {{Do Party Systems Count?: The Number of Parties and Government Performance in the Indian States}},
url = {http://cps.sagepub.com/cgi/doi/10.1177/0010414003260981},
volume = {37},
year = {2004}
