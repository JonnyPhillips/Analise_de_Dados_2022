@article{Shadish2008,
abstract = {A key justification for using nonrandomized experiments is that, with{\$}\backslash{\$}nproper adjustment, their results can well approximate results from{\$}\backslash{\$}nrandomized experiments. This hypothesis has not been consistently{\$}\backslash{\$}nsupported by empirical studies; however, previous methods used to{\$}\backslash{\$}nstudy this hypothesis have confounded assignment method with other{\$}\backslash{\$}nstudy features. To avoid these confounding factors, this study randomly{\$}\backslash{\$}nassigned participants to be in a randomized experiment or a nonrandomized{\$}\backslash{\$}nexperiment. In the randomized experiment, participants were randomly{\$}\backslash{\$}nassigned to mathematics or vocabulary training; in the nonrandomized{\$}\backslash{\$}nexperiment, participants chose their training. The study held all{\$}\backslash{\$}nother features of the experiment constant; it carefully measured{\$}\backslash{\$}npretest variables that might predict the condition that participants{\$}\backslash{\$}nchose, and all participants were measured on vocabulary and mathematics{\$}\backslash{\$}noutcomes. Ordinary linear regression reduced bias in the nonrandomized{\$}\backslash{\$}nexperiment by 84?94{\%} using covariate-adjusted randomized results{\$}\backslash{\$}nas the benchmark. Propensity score stratification, weighting, and{\$}\backslash{\$}ncovariance adjustment reduced bias by about 58?96{\%}, depending on{\$}\backslash{\$}nthe outcome measure and adjustment method. Propensity score adjustment{\$}\backslash{\$}nperformed poorly when the scores were constructed from predictors{\$}\backslash{\$}nof convenience (sex, age, marital status, and ethnicity) rather than{\$}\backslash{\$}nfrom a broader set of predictors that might include these.},
author = {Shadish, William R and Clark, M H and Steiner, Peter M},
doi = {10.1198/016214508000000733},
issn = {01621459},
journal = {Journal of the American Statistical Association},
keywords = {Nonrandomized experiment,Propensity score,Randomized experiment,Selection bias},
number = {484},
pages = {1334--1343},
title = {{Can nonrandomized experiments yield accurate answers? A randomized experiment comparing random and nonrandom assignments}},
volume = {103},
year = {2008}
}
