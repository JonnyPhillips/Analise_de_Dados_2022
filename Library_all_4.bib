journal = {Review of Economic Studies},
keywords = {Bias,Global games,Hidden actions,Media,Propaganda,Signal-jamming},
number = {4},
pages = {1422--1458},
pmid = {15991970},
title = {{Information manipulation, coordination, and regime change}},
volume = {80},
year = {2013}
}
