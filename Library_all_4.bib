number = {11},
pages = {1492--1514},
title = {{What You See and What You Get Pitfalls and Principles of Nested Analysis in Comparative Research}},
url = {http://cps.sagepub.com/content/41/11/1492{\%}5Cnhttp://cps.sagepub.com/content/41/11/1492.abstract},
volume = {41},
year = {2008}
}
