@article{Keele2015a,
abstract = {Ballot initiatives allow the public to vote directly on public policy. A literature in political science has attempted to document whether the presence of an initiative can increase voter turnout. Here, we study this question for an initiative that appeared on the ballot inMilwaukee,WI in 2008. We study this question using a natural experiment based on geography. This form of natural experiment exploits variation in geography where units in one geographic area receive a treatment while units in another area do not. The hope is that assignment to treatment via geographic location creates as-if ran- dom variation in treatment assignment. When this happens, adjustment for baseline covariates is unnecessary. In many applications, however, some adjustment for base- line covariates may be necessary. As such, analysts may wish to combine identification strategies–using both spatial proximity and covariates. We develop a matching algo- rithm to flexibly incorporate information about both the discontinuity and observed covariates which allows us to minimize spatial distance while preserving balance on observed covariates. Integrating these two strategies results in weaker assumptions for identification. Also, with this approach it is straightforward to conduct a sensitivity analysis to bias from hidden confounders.},
author = {Keele, Luke and Titiunik, Roc{\'{i}}o and Zubizarreta, Jos{\'{e}} R},
doi = {10.1111/rssa.12056},
issn = {1467985X},
journal = {Journal of the Royal Statistical Society. Series A: Statistics in Society},
keywords = {Ballot initiative,Matching,Regression discontinuity design},
number = {1},
pages = {223--239},
title = {{Enhancing a geographic regression discontinuity design through matching to estimate the effect of ballot initiatives on voter turnout}},
volume = {178},
year = {2015}
}
